{
  "name": "@damon945/git-quicker",
  "version": "3.0.1",
  "type": "module",
  "description": "一个简化和增强 Git 操作的 TypeScript 命令行工具，让您的 Git 工作流更加高效！",
  "repository": "https://github.com/zhouj945/Git-quicker",
  "bugs": {
    "url": "https://github.com/zhouj945/Git-quicker/issues"
  },
  "homepage": "https://github.com/zhouj945/Git-quicker#readme",
  "main": "dist/index.js",
  "bin": {
    "gq": "dist/cli.js",
    "git-quicker": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint \"src/**/*.ts\"",
    "lint:fix": "./node_modules/.bin/eslint \"src/**/*.ts\" --fix",
    "prepare": "npm run build",
    "prepublishOnly": "test \"$CI\" = \"true\" || (npm run build && npm test)",
    "release:prepare": "./scripts/prepare-release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "keywords": [
    "git",
    "cli",
    "productivity",
    "shortcuts",
    "version-control",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "inquirer": "^9.3.8",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "ora": "^5.4.1",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/inquirer": "^9.0.9",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
