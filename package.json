{
  "name": "@damon945/git-quicker",
  "version": "2.0.12",
  "description": "一个简化和增强 Git 操作的 TypeScript 命令行工具，让您的 Git 工作流更加高效！",
  "repository": "https://github.com/zhouj945/Git-quicker",
  "bugs": {
    "url": "https://github.com/zhouj945/Git-quicker/issues"
  },
  "homepage": "https://github.com/zhouj945/Git-quicker#readme",
  "main": "dist/index.js",
  "bin": {
    "gq": "dist/cli.js",
    "git-quicker": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "release:prepare": "./scripts/prepare-release.sh",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "keywords": [
    "git",
    "cli",
    "productivity",
    "shortcuts",
    "version-control",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
